{
  "name": "deel-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "start": "npm run build && nodemon ./dist/server.js",
    "seed": "npm run build && node ./dist/scripts/seedDb.js",
    "lint": "eslint '*/**/*.ts'",
    "clean": "rimraf dist",
    "dev": "concurrently 'nodemon' 'tsc --watch --preserveWatchOutput' -n node,ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "4.17.1",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/sequelize":"^4.28.19",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "engineStrict": true
